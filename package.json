    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.20.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "prettier": "2.5.1",
    "pretty-quick": "^3.1.3",
    "react-scripts": "5.0.0",
    "remark": "^14.0.2",
